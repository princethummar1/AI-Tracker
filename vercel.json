{
  "version": 2,
  "buildCommand": "npm install",
  "framework": null,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/server.js" },
    { "src": "/(.*)", "dest": "/index.html", "methods": ["GET"] }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
